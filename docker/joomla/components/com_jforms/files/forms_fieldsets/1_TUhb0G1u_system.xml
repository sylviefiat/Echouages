<?xml version="1.0" encoding="utf-8"?>


<form>

	<fieldset name="system"
		label="MY_DEMO1_SYSTEM"
		printable="true"
		>
				
		<field name="status"
				alias="status"
				label="MY_DEMO1_STATUS"
				description="MY_DEMO1_DESC"
				type="status"/>	
								
		<field name="templatestyle"
				alias="templatestyle"
				label="MY_DEMO1_TEMPLATESTYLE"
				description="MY_DEMO1_DESC"
				type="templatestyle"/>
				
				
		<field name="menu"
				alias="menu"
				label="MY_DEMO1_MENU"
				description="MY_DEMO1_DESC"
				type="menu"/>
				
		<field name="menuitem"
				alias="menuitem"
				label="MY_DEMO1_MENUITEM"
				description="MY_DEMO1_DESC"
				type="menuitem"/>
				
		<field name="moduletag"
				alias="moduletag"
				label="MY_DEMO1_MODULETAG"
				description="MY_DEMO1_DESC"
				type="moduletag"/>
				
		<field name="plugins"
				alias="plugins"
				label="MY_DEMO1_PLUGINS"
				description="MY_DEMO1_DESC"
