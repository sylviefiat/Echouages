				type="databaseconnection"/>
				
		<field name="joomla_headertag"
				alias="joomla_headertag"
				label="MY_DEMO1_HEADERTAG"
				description="MY_DEMO1_DESC"
				type="headertag"/>
				
				
		<field name="joomla_sessionhandler"
				alias="joomla_sessionhandler"
				label="MY_DEMO1_SESSIONHANDLER"
				description="MY_DEMO1_DESC"
				type="sessionhandler"/>
				
		<field name="joomla_registrationdaterange"
				alias="joomla_registrationdaterange"
				label="MY_DEMO1_REGISTRATIONDATERANGE"
				description="MY_DEMO1_DESC"
				type="registrationdaterange"/>
		
